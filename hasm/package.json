{
    "name": "hasm",
    "displayName": "hasm",
    "description": "Human Assembler",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "hasm",
            "aliases": ["HASM", "hasm"],
